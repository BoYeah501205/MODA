{
  "name": "moda-optimized",
  "version": "4.0.0",
  "description": "Modular Operations Dashboard Application - Fully Optimized",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run copy-assets",
    "copy-assets": "node -e \"const fs=require('fs');const path=require('path');function copy(s,d){if(!fs.existsSync(s))return;fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).forEach(f=>{const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?copy(sp,dp):fs.copyFileSync(sp,dp)})};copy('js','dist/js');copy('css','dist/css');copy('public','dist/public');console.log('Assets copied!')\"",
    "preview": "vite preview",
    "serve": "vite preview --port 8000",
    "type-check": "tsc --noEmit",
    "lint": "eslint js/**/*.{js,ts,tsx} --fix",
    "format": "prettier --write js/**/*.{js,ts,tsx,css,md}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "vite-bundle-analyzer dist/stats.json"
  },
  "dependencies": {
    "firebase": "^12.6.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^10.3.0",
    "react-router-dom": "^7.10.1",
    "tesseract.js": "^5.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.16.0",
    "workbox-window": "^7.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "optimization",
    "dashboard",
    "modular"
  ],
  "author": "MODA Optimization Team",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
